{
  "name": "xltxlm/thrift",
  "description": "apache thrift 0.10.0 扣出来的php代码,包含代码生成工具",
  "authors": [
    {
      "name": "xltxlm",
      "email": "xltxlm@qq.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "xltxlm/helper": "*"
  },
  "autoload": {
    "psr-0": {
      "Thrift": "lib/"
    },
    "psr-4": {
      "xltxlm\\thrift\\": [
        "./",
        "./src"
      ],
      "Thrift\\": "/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "shared\\": "tests/ThriftClient/shared/",
      "tutorial\\": "tests/ThriftClient/tutorial/",
      "sharednew\\": "tests/ThriftClient/sharednew/",
      "tutorialnew\\": "tests/ThriftClient/tutorialnew/",
      "": "./"
    }
  }
}
